{
  "expo": {
    "name": "EmlakÇantam",
    "slug": "emlakcantam",
    "version": "0.0.6",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "emlakcantam",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.emlakcantam.app",
      "buildNumber": "1",
      "infoPlist": {
        "NSContactsUsageDescription": "Rehberinizdeki kişileri müşteri olarak eklemek için izin gerekiyor."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.emlakcantam.app",
      "versionCode": 1,
      "permissions": ["android.permission.READ_CONTACTS"]
    },
    "web": {
      "bundler": "metro",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-notifications",
      [
        "expo-contacts",
        {
          "contactsPermission": "Rehberinizdeki kişileri müşteri olarak eklemek için izin gerekiyor."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "tsconfigPaths": true
    },
    "developmentClient": {
      "silentLaunch": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "c555549b-4f14-4e52-aaed-67b44061f304"
      }
    },
    "owner": "simbolmina",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://simbolmina.dev",
      "enabled": true
    },
    "newArchEnabled": true
  }
}
